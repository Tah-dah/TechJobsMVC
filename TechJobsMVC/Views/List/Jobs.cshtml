<div class="container body-content">

    <h1>@ViewBag.title</h1>
    
    <h3>@ViewBag.jobs.Count Results</h3> 
    
   @{
       @foreach (var job in ViewBag.jobs)
        {
            <table class="job-listing">

                <tr>
                    <th>
                        ID
                    </th>
                    <td>
                        @job.Id
                    </td>
                </tr>

                <tr>
                    <th>
                        Name
                    </th>
                    <td>
                        @job.Name
                    </td>
                </tr>

                <tr>
                    <th>
                        Employer
                    </th>
                    <td>
                        @job.Employer
                    </td>
                </tr>

                <tr>
                    <th>
                        Location
                    </th>
                    <td>
                        @job.Location
                    </td>
                </tr>

                <tr>
                    <th>
                        Position Type
                    </th>
                    <td>
                        @job.PositionType
                    </td>
                </tr>

                <tr>
                    <th>
                        CoreCompentency
                    </th>
                    <td>
                        @job.CoreCompetency
                    </td>
                </tr>

            </table>

